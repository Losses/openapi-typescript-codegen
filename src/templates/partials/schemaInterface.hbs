{
    properties: {
{{#if properties}}
    {{#each properties}}
        {{#if isReference}}
        {{{name}}}: ${{{base}}},
        {{else}}
        {{{name}}}: {{>schema}},
        {{/if}}
    {{/each}}
{{/if}}
    },
{{#if isReadOnly}}
    readOnly: {{{isReadOnly}}},
{{/if}}
{{#if isRequired}}
    required: {{{isRequired}}},
{{/if}}
{{#if isNullable}}
    nullable: {{{isNullable}}},
{{/if}}
}
