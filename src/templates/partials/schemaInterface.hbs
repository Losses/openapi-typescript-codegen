{
    type: 'object',
    properties: {
{{#if properties}}
    {{#each properties}}
        {{#if isReference}}
        {{{name}}}: ${{{base}}},
        {{else}}
        {{{name}}}: {{>schema}},
        {{/if}}
    {{/each}}
{{/if}}
    },
    required: [
        {{#each properties}}
        {{#if isRequired}}
        '{{{name}}}',
        {{/if}}
        {{/each}}
    ],
{{#if isReadOnly}}
    readOnly: {{{isReadOnly}}},
{{/if}}
{{#if isNullable}}
    nullable: {{{isNullable}}},
{{/if}}
}
