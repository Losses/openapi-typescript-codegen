{
    type: 'array',
{{#if link}}
    contains: {{>schema link}},
{{else}}
{{#if isReference}}
    contains: ${{{base}}},
{{else}}
    contains: {
        type: '{{{base}}}',
    },
{{/if}}
{{/if}}
{{#if isReadOnly}}
    readOnly: {{{isReadOnly}}},
{{/if}}
{{#if isNullable}}
    nullable: {{{isNullable}}},
{{/if}}
}
